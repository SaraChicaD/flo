{
  "name": "flo",
  "private": true,
  "version": "0.0.1",
  "description": "Health tracking app",
  "repository": "https://github.com/SaraChicaD/flo",
  "license": "MIT",
  "devDependencies": {
    "karma": "~0.10",
    "karma-junit-reporter": "latest",
    "protractor": "1.1.x",
    "http-server": "0.6.x",
    "bower": "1.3.x"
  },
  "scripts": {
    "postinstall": "bower install",
